<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <meta charset="utf-8">
    <title></title>
    <link rel="stylesheet" href="css/styles.css">
  </head>
  <body>


<div class="box" id="heading">
  <h1><%=kindofDay  %></h1>
</div>

<div class="box">
  <% newListItems.forEach((item) => {  %>
    <div class="item">
      <input type="checkbox" >
      <p><%= item %> </p>
    </div>
  <%});%>
  <form class = "item" action="/" method="post">
    <input type="text" name="texto" placeholder="New Item" autocomplete="off">
    <button type="submit"name="button" >+</button>
  </form>
</div>
</body>
</html>
